{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "builder": "swc",
    "typeCheck": true,
    "tsConfigPath": "tsconfig.build.json"
  },
  "projects": {
    "prisma": {
      "type": "library",
      "root": "prisma",
      "entryFile": "index",
      "sourceRoot": "prisma",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.json"
      }
    }
  }
}
